<header>
    <a href="#">calc</a>
    <p>theme</p>
    <div class="theme-switch-container">
        <div class="theme-switch">
            <div class="switch-numbers">
                <span>1</span>
                <span>2</span>
                <span>3</span>
            </div>
            <div class="switch-btns">
                <div class="input-container">
                    <input type="checkbox" class="theme-btn theme-1 active" />
                    <input type="checkbox" class="theme-btn theme-2" />
                    <input type="checkbox" class="theme-btn theme-3" />
                </div>
            </div>
        </div>
    </div>
</header>
